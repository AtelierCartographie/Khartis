{{#if (x-eq cType 'text')}}
<span class="icon-type {{if isValid 'text-success' 'text-warning'}}">abc</span>
{{else if (x-eq cType 'numeric')}}
<span class="icon-type {{if isValid 'text-success' 'text-warning'}}">#</span>
{{else if (x-eq cType 'geo')}}
<span class="icon-type geo {{if isValid 'text-success' 'text-warning'}}"><i class="iconfont iconfont-globe"></i></span>
{{else if (x-some (x-eq cType 'lat') (x-eq cType 'lat_dms'))}}
<span class="icon-type coord {{if isValid 'text-success' 'text-warning'}}">lat</span>
{{else if (x-some (x-eq cType 'lon') (x-eq cType 'lon_dms'))}}
<span class="icon-type coord {{if isValid 'text-success' 'text-warning'}}">long</span>
{{/if}}
